default: &default
  adapter: postgresql
  pool: <%= ENV.fetch("RAILS_MAX_THREADS") { 5 } %>
  username: postgres
  password: password
  host: db

development:
  <<: *default
  database: vegetable_service_development

test:
  <<: *default
  database: vegetable_service_test

production:
  <<: *default
  url: <%= ENV['DATABASE_URL'] %>
